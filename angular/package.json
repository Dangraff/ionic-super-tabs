{
  "name": "@ionic-super-tabs/angular",
  "version": "6.0.0-beta.8",
  "description": "Angular specific wrappers for @ionic-super-tabs/core",
  "module": "dist/fesm5.js",
  "main": "dist/fesm5.js",
  "types": "dist/core.d.ts",
  "files": [
    "dist/",
    "css/"
  ],
  "scripts": {
    "build": "npm run build.ng",
    "build.ng": "npm run build.es2015 && npm run build.es5",
    "build.es2015": "ngc -p tsconfig.json && rollup --config ./rollup.config.js",
    "build.es5": "ngc -p tsconfig.legacy.json && rollup --config ./rollup.config.legacy.js"
  },
  "author": "Zyra Media Inc. <info@zyra.ca>",
  "license": "MIT",
  "dependencies": {
    "@ionic-super-tabs/core": "^6.0.0-beta.8"
  },
  "devDependencies": {
    "@angular-devkit/core": "^7.2.1",
    "@angular-devkit/schematics": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "@types/node": "~10.12.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "rollup": "^1.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rxjs": "^6.2.0",
    "tsickle": "^0.34.0",
    "tslint": "^5.12.1",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "3.2.4",
    "zone.js": "^0.8.28"
  }
}
