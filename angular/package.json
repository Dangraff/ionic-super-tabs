{
  "name": "@ionic-super-tabs/angular",
  "version": "6.0.0-rc.0",
  "description": "Angular specific wrappers for @ionic-super-tabs/core",
  "module": "dist/fesm5.js",
  "main": "dist/fesm5.js",
  "types": "dist/core.d.ts",
  "files": [
    "dist/",
    "css/"
  ],
  "scripts": {
    "build": "npm run build.ng",
    "build.ng": "npm run build.es2015 && npm run build.es5",
    "build.es2015": "ngc -p tsconfig.json && rollup --config ./rollup.config.js",
    "build.es5": "ngc -p tsconfig.legacy.json && rollup --config ./rollup.config.legacy.js"
  },
  "author": "Zyra Media Inc. <info@zyra.ca>",
  "license": "MIT",
  "dependencies": {
    "@ionic-super-tabs/core": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@types/node": "~8.9.4",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "rollup": "^1.19.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rxjs": "~6.5.2",
    "tsickle": "^0.34.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "zone.js": "~0.9.1"
  }
}
